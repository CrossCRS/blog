{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "max-len": "off",
        "no-param-reassign": "off",
        "newline-per-chained-call": "off",
        "default-case": "off"
    }
}
